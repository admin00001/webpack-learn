{
  "name": "webpack-learn",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./dist/main.js",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:render": "cross-env NODE_ENV=production webpack --config webpack.render.config.js",
    "build": "run-p build:main build:render",
    "dev:render": "cross-env NODE_ENV=development webpack serve --config webpack.render.config.js",
    "dev:main": "cross-env NODE_ENV=development webpack --config webpack.main.config.js --watch",
    "dev": "run-p dev:main dev:render"
  },
  "keywords": [],
  "author": "",
